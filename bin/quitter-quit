#!/bin/bash
echo "#bash - Quitter quit start"
echo "#bash - Quitting omxplayer"
killall omxplayer
killall omxplayer.bin
echo "#bash - Quitting MP4Box"
killall MP4Box
echo "#bash - Quitting raspivid"
killall raspivid
echo "#bash - Quit playback killing mpg123"
killall mpg123
#killall mpg123.bin
echo "#bash - Quitter quit end"
